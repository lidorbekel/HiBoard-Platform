      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/platform-e2e/cypress.json",
            "tsConfig": "apps/platform-e2e/tsconfig.json",
            "devServerTarget": "platform:serve:cypress",
            "watch": true
          },
          "configurations": {
            "ci": {
              "devServerTarget": "",
              "watch": false
            }
          }
        },
        "lint": {
